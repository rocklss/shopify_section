e6 .smember-icons a:nth-child(2){-webkit-transition-delay:.3s;transition-delay:.3s}.smember--style6 .smember-icons a:nth-child(3){-webkit-transition-delay:.4s;transition-delay:.4s}.smember--style6 .smember-icons a:nth-child(4){-webkit-transition-delay:.5s;transition-delay:.5s}.smember--style6 .smember-info{padding-top:15px;background:#fff}.smember--style6 .smember-name{font-size:16px;margin:0}.smember--style6 span.smember-role{font-size:12px;display:block}.smember--style6:hover .smember-photo img{-webkit-transform:scale(1.1);transform:scale(1.1)}.smember--style6 .smember-icon{-webkit-transform:translateX(50px);transform:translateX(50px);-webkit-transition:-webkit-transform .3s cubic-bezier(0,0,0,.2),transform .3s cubic-bezier(0,0,0,.2);transition:-webkit-transform .3s cubic-bezier(0,0,0,.2),transform .3s cubic-bezier(0,0,0,.2);width:30px;margin-bottom:2px;display:inline-block}.smember--style6:hover .smember-icon{-webkit-transform:translateX(0);transform:translateX(0)}


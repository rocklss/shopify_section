<div class="steam-member__wrap">
  <div class="steam-member__content">
    {% if section.settings.title != blank %}
    <h2 class="steam-member__title">{{ section.settings.title }}</h2>
    {% endif %}
    {% if section.settings.subtitle != blank %}
    <h4 class="steam-member__subtitle">{{ section.settings.subtitle }}</h4>
    {% endif %}
    <div class="smembers">
      {% for block in section.blocks %}
      <div class="smember-wr smember-wr--{{section.settings.items}}">
        <div class="smember--style{{section.settings.layout}}">
          <div class="smember-photo">
            {% if block.settings.image != blank %}
            <img src="{{block.settings.image | img_url:'800x'}}" alt="{{block.settings.name | handle}}" />
            {% else %}
            {% capture current %}{% cycle 1, 2, 3, 4, 5, 6 %}{% endcapture %}
            {{ 'product-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}
            {% endif %}
            <div class="smember-icons">
              {% if block.settings.twitter != blank %}
              <a href="{{block.settings.twitter}}" title="{{block.settings.name}} in twitter" class="smember-icon">
                <svg aria-hidden="true" data-prefix="fab" data-icon="twitter" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"><path fill="currentColor" d="M459.37 151.716c.325 4.548.325 9.097.325 13.645 0 138.72-105.583 298.558-298.558 298.558-59.452 0-114.68-17.219-161.137-47.106 8.447.974 16.568 1.299 25.34 1.299 49.055 0 94.213-16.568 130.274-44.832-46.132-.975-84.792-31.188-98.112-72.772 6.498.974 12.995 1.624 19.818 1.624 9.421 0 18.843-1.3 27.614-3.573-48.081-9.747-84.143-51.98-84.143-102.985v-1.299c13.969 7.797 30.214 12.67 47.431 13.319-28.264-18.843-46.781-51.005-46.781-87.391 0-19.492 5.197-37.36 14.294-52.954 51.655 63.675 129.3 105.258 216.365 109.807-1.624-7.797-2.599-15.918-2.599-24.04 0-57.828 46.782-104.934 104.934-104.934 30.213 0 57.502 12.67 76.67 33.137 23.715-4.548 46.456-13.32 66.599-25.34-7.798 24.366-24.366 44.833-46.132 57.827 21.117-2.273 41.584-8.122 60.426-16.243-14.292 20.791-32.161 39.308-52.628 54.253z" class=""></path></svg>
              </a>
              {% endif %}
              {% if block.settings.google != blank %}
              <a href="{{block.settings.google}}" title="{{block.settings.name}} in google plus" class="smember-icon">
                <svg aria-hidden="true" data-prefix="fab" data-icon="google-plus-g" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 640 512"><path fill="currentColor" d="M386.061 228.496c1.834 9.692 3.143 19.384 3.143 31.956C389.204 370.205 315.599 448 204.8 448c-106.084 0-192-85.915-192-192s85.916-192 192-192c51.864 0 95.083 18.859 128.611 50.292l-52.126 50.03c-14.145-13.621-39.028-29.599-76.485-29.599-65.484 0-118.92 54.221-118.92 121.277 0 67.056 53.436 121.277 118.92 121.277 75.961 0 104.513-54.745 108.965-82.773H204.8v-66.009h181.261zm185.406 6.437V179.2h-56.001v55.733h-55.733v56.001h55.733v55.733h56.001v-55.733H627.2v-56.001h-55.733z" class=""></path></svg>
              </a>
              {% endif %}
              {% if block.settings.facebook != blank %}
              <a href="{{block.settings.facebook}}" title="{{block.settings.name}} in facebook" class="smember-icon">
                <svg aria-hidden="true" data-prefix="fab" data-icon="facebook-f" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 264 512"><path fill="currentColor" d="M76.7 512V283H0v-91h76.7v-71.7C76.7 42.4 124.3 0 193.8 0c33.3 0 61.9 2.5 70.2 3.6V85h-48.2c-37.8 0-45.1 18-45.1 44.3V192H256l-11.7 91h-73.6v229" class=""></path></svg>
              </a>
              {% endif %}
              {% if block.settings.linkedin != blank %}
              <a href="{{block.settings.linkedin}}" title="{{block.settings.name}} in linkedin" class="smember-icon">
                <svg aria-hidden="true" data-prefix="fab" data-icon="linkedin-in" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512"><path fill="currentColor" d="M100.3 448H7.4V148.9h92.9V448zM53.8 108.1C24.1 108.1 0 83.5 0 53.8S24.1 0 53.8 0s53.8 24.1 53.8 53.8-24.1 54.3-53.8 54.3zM448 448h-92.7V302.4c0-34.7-.7-79.2-48.3-79.2-48.3 0-55.7 37.7-55.7 76.7V448h-92.8V148.9h89.1v40.8h1.3c12.4-23.5 42.7-48.3 87.9-48.3 94 0 111.3 61.9 111.3 142.3V448h-.1z" class=""></path></svg>
              </a>
              {% endif %}
            </div>
          </div>
          {% if block.settings.name != blank %}
          <div class="smember-info">
            <h3 class="smember-name">{{block.settings.name}}</h3>
            <span class="smember-role">{{block.settings.position}}</span>
          </div>
          {% endif %}
        </div>
      </div>
      {% endfor %}
    </div>
  </div>
</div>

{% style %}

  {% comment %}========================   CSS Liquid ========================{% endcomment %}
  
  {% comment %} Gerenal setting {% endcomment %}
  {% assign title_font = section.settings.title_font %}
  {% assign subtitle_font = section.settings.subtitle_font %}
  {{ title_font | font_face }}
  {{ subtitle_font | font_face }}
  #shopify-section-{{section.id}} .steam-member__title{font-family:{{title_font.family}};color:{{section.settings.title_cl}};{{section.settings.title_style}}}
  #shopify-section-{{section.id}} .steam-member__subtitle{font-family:{{subtitle_font.family}};color:{{section.settings.subtitle_cl}};{{section.settings.subtitle_style}}}
  #shopify-section-{{section.id}} .steam-member__content{max-width:{{section.settings.max_width}}px;margin:0 auto;}
  #shopify-section-{{section.id}} .steam-member__wrap{margin-top:{{section.settings.margin_top}}px;margin-bottom:{{section.settings.margin_bottom}}px;padding-top:{{section.settings.padding_top}}px;padding-bottom:{{section.settings.padding_bottom}}px;background-color:{{section.settings.bg_color}};{% if section.settings.bg_image %}background-image:url({{section.settings.bg_image | img_url:'master'}});{% if section.settings.bg_attachment %}background-attachment:fixed;{% endif %}background-repeat:no-repeat;background-position:center;{% endif %}}
  
  #shopify-section-{{section.id}} .smember--style1 .smember-icons:before {background:{{section.settings.main_color}}}
  #shopify-section-{{section.id}} .smember--style1 .smember-icons {background:{{section.settings.main_color}}}
  #shopify-section-{{section.id}} .smember--style2 .smember-icons {background:{{section.settings.main_color}}}
  #shopify-section-{{section.id}} .smember--style2 .smember-icons:before {border-right-color:{{section.settings.main_color}}}
  #shopify-section-{{section.id}} .smember--style3 .smember-icons {background:{{section.settings.main_color}}}
  #shopify-section-{{section.id}} .smember--style4 .smember-info {background:{{section.settings.main_color}}}
  #shopify-section-{{section.id}} .smember--style4 .smember-icon:hover svg{color:{{section.settings.main_color}}}
  #shopify-section-{{section.id}} .smember--style5 .smember-icon{border-right: 2px solid {{section.settings.main_color}}}
  #shopify-section-{{section.id}} .smember--style5 .smember-icon svg{color:{{section.settings.main_color}}}
  #shopify-section-{{section.id}} .smember--style5 .smember-icon:hover{background:{{section.settings.main_color}}}
  #shopify-section-{{section.id}} .smember--style6 .smember-icon{color:{{section.settings.main_color}};background:{{section.settings.main_color}}}
  
  {% comment %}========================   Pure CSS  ========================{% endcomment %}
  .smember--style1,.smember--style2,.smember--style3{box-shadow:0 10px 40px 0 rgba(0,0,0,.08)}.smembers{display:-webkit-box;display:-ms-flexbox;display:flex;-ms-flex-wrap:wrap;flex-wrap:wrap}.smember-photo{text-align:center}.smember-icon svg{height:15px;vertical-align:middle;margin:0 6px;color:#fff}@media (min-width:1200px){.smembers{margin:0 -10px}}.smember-wr{padding:10px}.smember-wr--2{-webkit-box-flex:0;-ms-flex:0 0 50%;flex:0 0 50%;max-width:50%}.smember-wr--3{-webkit-box-flex:0;-ms-flex:0 0 33.333333%;flex:0 0 33.333333%;max-width:33.333333%}.smember-wr--4{-webkit-box-flex:0;-ms-flex:0 0 25%;flex:0 0 25%;max-width:25%}@media (min-width:768px) and (max-width:1199px){.smember-wr{-webkit-box-flex:0;-ms-flex:0 0 50%;flex:0 0 50%;max-width:50%}}@media (max-width:767px){.smember-wr{-webkit-box-flex:0;-ms-flex:0 0 100%;flex:0 0 100%;max-width:100%}}.smember--style1{overflow:hidden}.smember--style1 .smember-photo{overflow:hidden;position:relative}.smember--style1 .smember-photo img{transition:all .3s cubic-bezier(0,0,0,.1);-webkit-transform:scale(1);transform:scale(1)}.smember--style1 .smember-icons{margin-top:10px;position:absolute;bottom:20px;width:70%;background:rgba(255,255,255,.13);text-align:center;padding:6px 20px}.smember--style1 .smember-icons:before{content:'';position:absolute;right:-100%;height:1px;width:100%;background:rgba(255,255,255,.13);top:50%}.smember--style1 .smember-info{padding:25px 20px}.smember--style1 .smember-name{font-size:18px;margin:0}.smember--style1 span.smember-role{font-size:12px;color:#999;display:block}.smember--style1:hover .smember-photo img{-webkit-transform:scale(1.1);transform:scale(1.1)}.smember--style2{-webkit-transform:translateY(0);transform:translateY(0);-webkit-transition:all .25s cubic-bezier(.43,.44,.63,.61);transition:all .25s cubic-bezier(.43,.44,.63,.61)}.smember--style2 .smember-photo{position:relative}.smember--style2 .smember-icons{position:absolute;top:0;text-align:center;width:40px;padding:10px 0;left:-7px}.smember--style2 .smember-icons:before{content:'';position:absolute;left:-7px;bottom:-7px;border:7px solid transparent}.smember--style2 .smember-info{padding:20px}.smember--style2 .smember-name{font-size:18px;margin:0;position:relative;padding-bottom:3px}.smember--style2 .smember-name:before{content:'';position:absolute;right:0;height:1px;width:30px;background:#000;bottom:0}.smember--style4 .smember-icon,.smember--style5 .smember-icon,.smember--style6 .smember-icon{height:30px;line-height:30px;text-align:center}.smember--style2 span.smember-role{font-size:12px;color:#999;display:block}.smember--style2:hover{-webkit-transform:translateY(-10px);transform:translateY(-10px)}.smember--style3{-webkit-transform:translateY(0);transform:translateY(0);-webkit-transition:all .25s cubic-bezier(.43,.44,.63,.61);transition:all .25s cubic-bezier(.43,.44,.63,.61)}.smember--style3 .smember-photo{position:relative}.smember--style3 .smember-icons{position:absolute;bottom:0;background:rgba(255,255,255,.1);width:100%;padding:7px 10px;left:0}.smember--style3 .smember-info{padding:20px}.smember--style3 .smember-name{font-size:18px;margin:0}.smember--style3 span.smember-role{font-size:12px;color:#999;display:block}.smember--style3:hover{-webkit-transform:translateY(-10px);transform:translateY(-10px)}.smember--style3 .smember-icon i:hover{background:#fff;color:#444}.smember--style4{position:relative;-webkit-transform:translateY(0);transform:translateY(0);-webkit-transition:all .15s cubic-bezier(0,0,0,.1);transition:all .15s cubic-bezier(0,0,0,.1);margin-bottom:51px}.smember--style4 .smember-icons{position:absolute;top:100%;padding:12px 15px;right:0;width:100%;-webkit-box-shadow:0 10px 40px 0 rgba(0,0,0,.08);box-shadow:0 10px 40px 0 rgba(0,0,0,.08)}.smember--style4 .smember-info{padding:8px 20px;position:absolute;bottom:0;width:100%;color:#fff;background:rgba(255,255,255,.02)}.smember--style4 .smember-name{font-size:18px;margin:0;color:#fff}.smember--style4 span.smember-role{font-size:12px;color:#